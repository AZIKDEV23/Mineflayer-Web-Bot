{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/connect",
      "methods": ["POST"],
      "dest": "public/server.js"
    },
    {
      "src": "/disconnect",
      "methods": ["POST"],
      "dest": "public/server.js"
    }
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ]
}
